<h1>画像投稿フォーム</h1>
  <%= form_with model: @item,url: admin_items_path(@item.id) do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>ショップ名</h4>
      <%= f.text_field :name %>
    <h4>説明</h4>
      <%= f.text_area :description %>
       <h4>税抜価格</h4>
      <%= f.text_field :price %>
    <%= f.submit '投稿' %>
  <% end %>