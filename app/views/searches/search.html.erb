<h2>Results index</h2>

<table class="table table-hover table-inverse">
  <tbody>
        <tr>
          <th>商品画像</th>
          <th>商品名</th>
          <th>商品価格</th>
          </tr>
      <% @items.each do |item| %>
        <tr>
          <td><%=  image_tag item.image.last ,size: "100x100" %></td>
          <td><%= item.name %></td>
          <td><%= item.price %>円（税抜き）</td>
        </tr>
      <% end %>
    </tbody>
</table>